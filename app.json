{
  "expo": {
    "name": "vad",
    "slug": "vad-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vadmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vadminingspace.vadmobile",
      "googleMobileAdsAppId": "ca-app-pub-4533962949749202~XXXXXXXXXX"
    },
    "android": {
      "package": "com.vadminingspace.vadmobile",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["INTERNET", "POST_NOTIFICATIONS"],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "googleMobileAdsAppId": "ca-app-pub-4533962949749202~8711232097"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": { "backgroundColor": "#000000" }
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-build-properties",
      "./plugins/withAdmobAppId",
      "react-native-google-mobile-ads"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "reactNativeGoogleMobileAdsAppId": "ca-app-pub-4533962949749202~8711232097",
      "eas": {
        "projectId": "8e9af1cf-e5b7-4272-9aaa-fc3b86afff37"
      }
    },
    "owner": "skima112"
  }
}
